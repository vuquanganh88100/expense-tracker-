<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ltnc.CategoryController">
   <children>
       <TableView fx:id="categoryTableView" layoutX="-1.0" layoutY="84.0" prefHeight="317.0" prefWidth="600.0">
           <columns>
               <TableColumn fx:id="idColumn" prefWidth="58.0" text="ID" />
               <TableColumn fx:id="nameColumn" prefWidth="197.0" text="Category Name" />
               <TableColumn fx:id="typeColumn" prefWidth="166.0" text="Category Type" />
               <TableColumn fx:id="actionColumn" prefWidth="178.0" text="Action" />
           </columns>
       </TableView>
       <Button layoutX="14.0" layoutY="24.0" mnemonicParsing="false" onAction="#addCategory" text="Add new category" />
   </children>
</AnchorPane>
